{
  "name": "gatsby-ts-boilerplate",
  "private": true,
  "description": "Gatsby TS starter with Styled Components + ESLint",
  "version": "1.2.0",
  "author": "Krzysztof Hankiewicz @kjhank <kjhank@gmail.com>",
  "dependencies": {
    "gatsby": "^5.3.2",
    "gatsby-plugin-image": "^3.3.2",
    "gatsby-plugin-manifest": "^5.3.1",
    "gatsby-plugin-offline": "^6.3.1",
    "gatsby-plugin-sharp": "^5.3.2",
    "gatsby-plugin-styled-components": "^6.3.0",
    "gatsby-plugin-typescript": "^5.3.1",
    "gatsby-source-filesystem": "^5.3.1",
    "gatsby-transformer-sharp": "^5.3.1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanitize-html": "^2.8.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/sanitize-html": "^2.6.2",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "gatsby-plugin-eslint": "^4.0.3",
    "gatsby-plugin-tsconfig-paths": "^1.0.5",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss-scss": "^4.0.6",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "styled-components",
    "eslint"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint . && stylelint './src/**/*.ts'",
    "lint:css": "stylelint './src/**/*.styled.ts'",
    "lint:ts": "eslint .",
    "lint:types": "tsc --noEmit",
    "nuke": "gatsby clean && rm -fr node_modules && yarn install",
    "pre-commit": "lint-staged",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "start": "yarn run develop",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjhank/gatsby-styled-components"
  },
  "bugs": {
    "url": "https://github.com/kjhank/gatsby-styled-components/issues"
  },
  "packageManager": "yarn@3.1.0"
}
